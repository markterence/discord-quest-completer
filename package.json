{
  "name": "tauri-app",
  "private": true,
  "version": "25.4.18",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "tauri": "tauri",
    "build:runner:win": "(cd ./src-win && cargo build --release)",
    "copy:runner:win": "cp ./src-win/target/release/src-win.exe ./src-tauri/resources/src-win.exe",
    "copy:resources:debug": "cp ./src-win/target/release/src-win.exe ./src-tauri/target/debug/data/src-win.exe",
    "copy:resources": "cp ./src-win/target/release/src-win.exe ./src-tauri/target/release/data/src-win.exe",
    "sync:runner": "pnpm run build:runner:win && pnpm run copy:runner:win && pnpm copy:resources",
    "sync:runner:dev": "pnpm run build:runner:win && pnpm run copy:runner:win && pnpm copy:resources && pnpm run copy:resources:debug && pnpm run view-size",
    "view-size": "ls -lh ./src-win/target/release/src-win.exe",
    "build:all": "tauri build && pnpm run build:template && pnpm run copy:template",
    "tauri:dev": "tauri dev && pnpm run copy:template"
  },
  "dependencies": {
    "@tailwindcss/vite": "^4.1.3",
    "@tauri-apps/api": "^2.6.0",
    "@tauri-apps/plugin-fs": "~2",
    "@tauri-apps/plugin-http": "~2.5.0",
    "@tauri-apps/plugin-opener": "^2.3.1",
    "@tauri-apps/plugin-sql": "~2.2.1",
    "@vueuse/core": "^13.1.0",
    "lodash-es": "^4.17.21",
    "nanoid": "^5.1.5",
    "tailwindcss": "^4.1.3",
    "unplugin-vue-router": "^0.15.0",
    "vue": "^3.5.13",
    "vue-router": "4"
  },
  "devDependencies": {
    "@tauri-apps/cli": "^2.6.0",
    "@types/lodash-es": "^4.17.12",
    "@vitejs/plugin-vue": "^5.2.1",
    "typescript": "~5.6.2",
    "vite": "^6.0.3",
    "vue-tsc": "^2.1.10"
  },
  "packageManager": "pnpm@8.15.3+sha256.fc4a49bd609550a41e14d20efbce802a4b892aa4cac877322de2f0924f122991"
}
